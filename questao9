#include <stdio.h>
//Calcular e mostrar a média aritmética dos números pares compreendidos entre 13 e 73
int main(void) {
  int i, j, par=0, div=0, som1=0, som2=0;
  for(i=13; i<=73 ; i++){
    par=i;
    div=1;
   if(par%2==0){
     par=i;
     div=1;
   }
   else{
     div=0;
     par=0;
   }
   som1+=par;
   som2+=div;
}
 printf("%d",som1/som2);
}
