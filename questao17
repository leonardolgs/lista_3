#include <stdio.h>
//Crie uma função para converter de Celsius para Farenheit
float converter(){
  float c, f;
  float resultado;
  printf("digite quanros graus são: ");
  scanf("%f", &c);
  f = (c*(9/5))+32; 
  return f;
}

int main(void) {
  float a = converter();
  printf("%3.2f", a );
  return 0;
}
